{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["gluetun"],
      "versioning": "semver"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["minio"],
      "versioning": "regex:^RELEASE\\.(?<major>\\d+)-(?<minor>\\d+)-(?<patch>\\d+)T.*Z$"
    }
  ]
}
