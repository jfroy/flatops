{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["gluetun"],
      "versioning": "semver"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["plex"],
      "versioning": "loose"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePrefixes": ["registry.kantai.xyz/"],
      "enabled": false
    }
  ]
}
